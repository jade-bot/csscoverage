#!/usr/bin/env node

var args = process.argv.slice(2);


var print = function(results) {
  results.forEach(function(sheet, i) {
    console.log(sheet.cssFile);
    console.log('-------------------------------------');
    sheet.missing.forEach(function(selector, i) {
      console.log(selector);
    });
    console.log('');
    console.log('');
  });
};


if (args.length == 1) {
  var lonely = require('../lib/lonelystyles');
  var url = args[0];
  lonely.run(url, function(results) {
    print(results);
  });
} else {
  console.log("Please pass in url");
}
